{
  "name": "unplugin-dingtalk",
  "type": "module",
  "version": "1007.0.1",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/zcf0508/unplugin-dingtalk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zcf0508/unplugin-dingtalk.git"
  },
  "bugs": {
    "url": "https://github.com/zcf0508/unplugin-dingtalk/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint .",
    "play": "npm -C playground/vite run dev",
    "play:nuxt": "npm -C playground/nuxt run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "test": "vitest"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "@rsbuild/core": "^0.4.3",
    "@rspack/core": "^0.5.4",
    "esbuild": "*",
    "proper-lockfile": "^4.1.2",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "@rsbuild/core": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "vite-plugin-vconsole": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "cookie": "^1.0.2",
    "h3": "^1.15.4",
    "http-proxy": "^1.18.1",
    "picocolors": "^1.0.1",
    "unplugin": "2.2.0",
    "z-chii": "1.15.5-beta.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@nuxt/kit": "^3.8.2",
    "@nuxt/schema": "^3.8.2",
    "@types/http-proxy": "^1.17.16",
    "@types/node": "^20.10.3",
    "@types/proper-lockfile": "^4.1.4",
    "bumpp": "^9.2.0",
    "chalk": "^5.3.0",
    "eslint": "^9.38.0",
    "eslint-plugin-security": "^3.0.1",
    "fast-glob": "^3.3.2",
    "get-port-please": "^3.1.2",
    "nodemon": "^3.0.2",
    "proper-lockfile": "^4.1.2",
    "rimraf": "^5.0.5",
    "rolldown": "1.0.0-beta.34",
    "rollup": "^4.6.1",
    "tsdown": "0.14.2",
    "typescript": "~5.7.3",
    "vite": "^5.0.4",
    "vite-plugin-vconsole": "^2.1.1",
    "vitest": "^1.6.0",
    "webpack": "^5.89.0",
    "webpack-dev-server": "^5.0.4"
  },
  "pnpm": {
    "overrides": {
      "rolldown": "1.0.0-beta.34"
    }
  }
}
